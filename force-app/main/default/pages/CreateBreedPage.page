<apex:page controller="Breed_CC" lightningStylesheets="true">
<apex:form>
    <apex:pageBlock title="Enter Breed Information">
        <apex:pageBlockSection collapsible="false" columns="2"> 
            <apex:inputField value="{!Br.Name}"/>
            <apex:inputField value="{!Br.Coat_Length__c}"/>
            <apex:inputField value="{!Br.Coat_Type__c}"/>
            <apex:inputField value="{!Br.Energy_Level__c}"/>
            <apex:inputField value="{!Br.Trainability_Level__c}"/>
            <apex:inputField value="{!Br.Height__c}"/>
            <apex:inputField value="{!Br.Weight__c}"/>
            <apex:inputField value="{!Br.Barking_Level__c}"/>
            <apex:inputField value="{!Br.Good_With_Kids__c}"/>
            <apex:inputField value="{!Br.Good_With_Other_Dogs__c}"/>
            <apex:inputField value="{!Br.Life_Expectancy__c}"/>
        </apex:pageBlockSection>
        <apex:pageBlockButtons>
            <apex:commandButton value="Save Breed" action="{!saveBreed}"/>
            <apex:commandButton value="Cancel" action="{!breedPage}"/>
        </apex:pageBlockButtons>
    </apex:pageBlock>
</apex:form>
</apex:page>